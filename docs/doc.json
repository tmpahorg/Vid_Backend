{
  "basePath": "/",
  "definitions": {
    "LoginDto": {
      "type": "object",
      "required": [
        "user",
        "token"
      ],
      "description": "登录信息",
      "properties": {
        "user": {
          "required": true,
          "description": "用户信息",
          "originRef": "UserDto",
          "$ref": "#/definitions/UserDto"
        },
        "token": {
          "type": "string",
          "required": true,
          "description": "登录令牌"
        }
      }
    },
    "LoginParam": {
      "type": "object",
      "required": [
        "username",
        "password"
      ],
      "description": "登录请求参数",
      "properties": {
        "username": {
          "type": "string",
          "required": true,
          "description": "用户名"
        },
        "password": {
          "type": "string",
          "required": true,
          "description": "密码"
        }
      }
    },
    "PasswordParam": {
      "type": "object",
      "required": [
        "password"
      ],
      "description": "修改密码请求参数",
      "properties": {
        "password": {
          "type": "string",
          "required": true,
          "description": "密码，长度在 [8, 30] 之间"
        }
      }
    },
    "PolicyDto": {
      "type": "object",
      "required": [
        "role",
        "path",
        "method"
      ],
      "description": "权限策略信息",
      "properties": {
        "role": {
          "type": "string",
          "required": true,
          "description": "角色 (sub)"
        },
        "path": {
          "type": "string",
          "required": true,
          "description": "路径 (obj)"
        },
        "method": {
          "type": "string",
          "required": true,
          "description": "方法 (act)"
        }
      }
    },
    "PolicyParam": {
      "type": "object",
      "required": [
        "role",
        "path",
        "method"
      ],
      "description": "权限策略参数",
      "properties": {
        "role": {
          "type": "string",
          "required": true,
          "description": "角色"
        },
        "path": {
          "type": "string",
          "required": true,
          "description": "路径"
        },
        "method": {
          "type": "string",
          "required": true,
          "description": "方法"
        }
      }
    },
    "RegisterParam": {
      "type": "object",
      "required": [
        "username",
        "password"
      ],
      "description": "注册请求参数",
      "properties": {
        "username": {
          "type": "string",
          "required": true,
          "description": "用户名，长度在 [5, 30] 之间"
        },
        "password": {
          "type": "string",
          "required": true,
          "description": "密码，长度在 [8, 30] 之间"
        }
      }
    },
    "Result": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "description": "global response",
      "properties": {
        "code": {
          "type": "integer",
          "required": true,
          "description": "status code",
          "format": "int32"
        },
        "message": {
          "type": "string",
          "required": true,
          "description": "status message"
        }
      }
    },
    "RoleParam": {

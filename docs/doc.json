      "description": "视频信息",
      "properties": {
        "vid": {
          "type": "integer",
          "required": true,
          "description": "视频id",
          "format": "int32"
        },
        "title": {
          "type": "string",
          "required": true,
          "description": "标题"
        },
        "description": {
          "type": "string",
          "required": true,
          "description": "简介",
          "allowEmptyValue": true
        },
        "video_url": {
          "type": "string",
          "required": true,
          "description": "资源"
        },
        "cover_url": {
          "type": "string",
          "required": true,
          "description": "封面"
        },
        "upload_time": {
          "type": "string",
          "required": true,
          "description": "上传时间",
          "format": "date-time"
        },
        "update_time": {
          "type": "string",
          "required": true,
          "description": "修改时间",
          "format": "date-time"
        },
        "author": {
          "required": true,
          "description": "视频作者, null 表示用户不存在",
          "originRef": "UserDto",
          "$ref": "#/definitions/UserDto"
        }
      }
    },
    "VideoParam": {
      "type": "object",
      "required": [
        "title",
        "description",
        "cover_url",
        "video_url"
      ],
      "description": "视频请求参数",
      "properties": {
        "title": {
          "type": "string",
          "required": true,
          "description": "标题，长度在 [1, 100] 之间"
        },
        "description": {
          "type": "string",
          "required": true,
          "description": "简介，长度在 [0, 255] 之间",
          "allowEmptyValue": true
        },
        "cover_url": {
          "type": "string",
          "required": true,
          "description": "封面"
        },
        "video_url": {
          "type": "string",
          "required": true,
          "description": "资源"
        }
      }
    },

      "description": "用户详细信息",
      "properties": {
        "user": {
          "required": true,
          "description": "用户信息",
          "originRef": "UserDto",
          "$ref": "#/definitions/UserDto"
        },
        "extra": {
          "required": true,
          "description": "用户额外信息",
          "originRef": "UserExtraDto",
          "$ref": "#/definitions/UserExtraDto"
        }
      }
    },
    "UserDto": {
      "type": "object",
      "required": [
        "uid",
        "username",
        "sex",
        "profile",
        "avatar_url",
        "birthday",
        "role",
        "register_time"
      ],
      "description": "用户信息",
      "properties": {
        "uid": {
          "type": "integer",
          "required": true,
          "description": "用户id",
          "format": "int32"
        },
        "username": {
          "type": "string",
          "required": true,
          "description": "用户名"
        },
        "sex": {
          "type": "string",
          "required": true,
          "description": "性别",
          "enum": [
            "male",
            "female",
            "unknown"
          ]
        },
        "profile": {
          "type": "string",
          "required": true,
          "description": "简介",
          "allowEmptyValue": true
        },
        "avatar_url": {
          "type": "string",
          "required": true,
          "description": "头像"
        },
        "birthday": {
          "type": "string",
          "required": true,
          "description": "生日",
          "format": "date"
        },
        "role": {
          "type": "string",
          "required": true,
          "description": "角色"
        },
        "register_time": {
          "type": "string",
          "required": true,
          "description": "注册时间",
          "format": "date-time"
        }
      }
    },
    "UserExtraDto": {
      "type": "object",
      "required": [
        "subscribing_cnt",
        "subscriber_cnt",
        "video_cnt"
      ],
      "description": "用户额外信息",
      "properties": {
        "subscribing_cnt": {
          "type": "integer",
          "required": true,
          "description": "关注数量",
          "format": "int32"
        },
        "subscriber_cnt": {
          "type": "integer",
          "required": true,
          "description": "粉丝数量",
          "format": "int32"
        },
        "video_cnt": {
          "type": "integer",
          "required": true,
          "description": "视频数量",
          "format": "int32"
        }
      }
    },
    "UserParam": {
      "type": "object",
      "required": [
        "username",
        "profile",
        "sex",
        "birthday",
        "phone_number",
        "avatar_url"
      ],
      "description": "用户请求参数",
      "properties": {
        "username": {
          "type": "string",
          "required": true,
          "description": "用户名，长度在 [5, 30] 之间"
        },
        "profile": {
          "type": "string",
          "required": true,
          "description": "用户简介，长度在 [0, 255] 之间",
          "allowEmptyValue": true
        },
        "sex": {
          "type": "string",
          "required": true,
          "description": "性别",
          "enum": [
            "male",
            "female",
            "unknown"
          ]
        },
        "birthday": {
          "type": "string",
          "required": true,
          "description": "生日",
          "format": "date"
        },
        "phone_number": {
          "type": "string",
          "required": true,
          "description": "手机号码，长度为 11，仅限中国大陆手机号码"
        },
        "avatar_url": {
          "type": "string",
          "required": true,
          "description": "头像"
        }
      }
    },
    "VideoDto": {
      "type": "object",
      "required": [
        "vid",
        "title",
        "description",
        "video_url",
        "cover_url",
        "upload_time",
        "update_time",
        "author"
